{
  "manifest_version": 3,
  "name": "Poznote URL Saver",
  "version": "1.3",
  "description": "Sauvegarde l'URL + capture d'Ã©cran dans Poznote",
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "icon.png"
    }
  },
  "icons": {
    "128": "icon.png"
  }
}